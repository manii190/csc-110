def remove_middle(list):
    if len(list)>0:
        if len(list)%2==0:
            middle=len(list)/2
            list.remove(middle)
        else:
            middle=(len(list)-1)/2
            print(middle)
            print(int(middle))
            list.pop(int(middle)+1)
    return list
print(remove_middle([1,2,3,4,5]))
