"""
def concatenate_lists(strings_a,strings_b):
    new_lists=[]
    for i in range(min(len(strings_a),len(strings_b))):
        new_lists.append(strings_a[i]+strings_b[i])
    if len(strings_a)<len(strings_b):
        return new_lists + strings_b[len(strings_a):]
    else:
        return new_lists + strings_a[len(strings_b):]
print(concatenate_lists([12,3,213,12,3,123],[3,123,12312,3,13,75]))
