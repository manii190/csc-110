def max_lists(numbers):
    maxi = None
    for i in range(len(numbers)):
        for j in range(len(numbers[i])):
                if maxi == None or numbers[i][j] > max_lists:
                    max_lists = numbers[i][j]
    return maxi

print(max_lists(([[], []])))
